<form data-type='event'>
   <div class="form-group row">
     <label class="col-form-label col-sm-2">Name</label>
     <div class='col-sm-10'>
     <input class="form-control" type="text" placeholder="New School Event 1" id="name" required>
     </div>
   </div>
   <div class="form-group row">
      <label class="col-form-label col-sm-2">Start Date</label>
     <div class='col-sm-10'>
       <input class="form-control" type="datetime-local" id="timestamp" required>
     </div>
   </div>
   <div class="form-group row">
     <label class="col-form-label col-sm-2">End Date</label>
    <div class='col-sm-10'>
       <input class="form-control" type="datetime-local" id="endDate" required>
    </div>
   </div>
   <div class="form-group row">
     <label for="exampleSelect1" class='col-sm-2'>Type</label>
     <div class='col-sm-10'>
     <select class="form-control" id="type">
       <option>Conference</option>
       <option>Competition</option>
       <option>Group</option>
       <option>Exhibition</option>
       <option>Meetup</option>
       <option>Hackathon</option>
       <option>Other</option>
     </select>
     </div>
   </div>
    <div class="form-group row">
     <label class="col-form-label col-sm-2">Focus</label>
     <div class='col-sm-10'>
     <input class="form-control" type="text" id="focus" required>
     </div>
   </div>
   <div class="form-group row">
     <label class="col-form-label col-sm-2">Location</label>
     <div class='col-sm-10'>
       <input class="form-control" type="text"  id="location" required>
     </div>
   </div>
   <div class="form-group row">
     <label class="col-form-label col-sm-2">Link</label>
     <div class='col-sm-10'>
       <input class="form-control" type="text" placeholder="http://www.abc.xyz" id="url" required>
     </div>
   </div>
   <div class="form-group row">
     <label class="col-sm-2" >Notes</label>
     <div class='col-sm-10'>
     <textarea class="form-control" id="notes" rows="3" ></textarea>
     </div>
   </div>
   <div class="form-group row">
     <label class="col-form-label col-sm-2">Your Name</label>
     <div class='col-sm-10'>
       <input class="form-control" type="text" id="from" required>
      </div>
   </div>
   <div class="form-group row">
     <label class="col-form-label col-sm-2">Your Email</label>
     <div class='col-sm-10'>
       <input class="form-control" type="text" id="fromEmail" required>
     </div>
   </div>
   <div class="form-group row">
   <label class="col-sm-2" ></label>
   <div class='col-sm-10'>
   <input type="submit" class="btn btn-primary" value="Submit"></input>
   </div>
   </div>
 </form>