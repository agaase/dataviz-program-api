{{#each this}}
  <div class='post'>
    <div class='content'>
      <div class='primary'>
      <div class='name'>{{title}}</div>
      </div>
      <div class='secondary'><span class='from'>{{from}}</span>, {{timestamp}}</div>
      <div class='desc'>{{{content}}}</div>
    </div>
  </div>
{{/each}}