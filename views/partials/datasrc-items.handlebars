{{#each this}}
{{#if nocontent}}
<div class='post'>
 <div class='primary'>
      <div class='name'>Nothing here right now!</div>
 </div>
</div>
{{else}}
{{#if title}}
  <div class='item'>
    <div class='content'>
      <a  href='{{link}}' target="_blank" class='name'>{{title}}</a>
      <div class='desc'>
      	<div class='descr'>{{descr}}</div>
      </div>
      <div class='attrs'>
        {{#if datasrc}}
         <div class='attr'>Data Source</div>
        {{else if datares}} 
        <div class='attr'>Resource/Tool</div>
        {{/if}}
      </div>
    </div>
  </div>
 {{/if}}
{{/if}}
{{/each}}